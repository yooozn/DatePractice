	$AnimationPlayer.play(\"colors\")
	Input.set_mouse_mode(Input.MOUSE_MODE_HIDDEN)

func _process(delta):
	if state == 1:
		time += delta
		$DateText2.visible = true
		$DateText2.text = str(stepify(time,.1))
		$CenterContainer2/DayWeek.visible = false
	else:
		$DateText2.visible = true


func _on_Button_pressed():
